meta {
  name: Get User
  type: http
  seq: 2
  tags: ["users"]
}

get {
  url: {{baseUrl}}/users/{{userId}}
  auth: none
}

headers {
  X-API-Key: {{apiKey}}
}

tests {
  test("200 ok", function() { expect(res.status).to.equal(200); });
  test("user data", function() {
    expect(res.body.user_id).to.equal('u-1001');
    expect(res.body.email).to.exist;
  });
}
