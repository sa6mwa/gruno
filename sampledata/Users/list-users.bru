meta {
  name: List Users
  type: http
  seq: 2
  tags: ["users"]
}

get {
  url: {{baseUrl}}/users
  auth: none
}

headers {
  X-API-Key: {{apiKey}}
}

tests {
  test("200 ok", function() { expect(res.status).to.equal(200); });
  test("has created user", function() {
    const arr = res.body;
    const ids = arr.map(u => u.user_id);
    expect(ids.includes('u-1001')).to.equal(true);
  });
}
