meta {
  name: Update User
  type: http
  seq: 3
  tags: ["users"]
}

patch {
  url: {{baseUrl}}/users/{{userId}}
  body: json
  auth: none
}

headers {
  X-API-Key: {{apiKey}}
  Content-Type: application/json
}

body:json {
  {
    "name": "Alice Updated",
    "email": "alice.updated@example.test"
  }
}

tests {
  test("200 ok", function() { expect(res.status).to.equal(200); });
  test("name updated", function() { expect(res.body.name).to.equal('Alice Updated'); });
}
