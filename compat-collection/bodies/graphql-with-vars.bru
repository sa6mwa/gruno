meta {
  name: GraphQL with vars
  type: http
  seq: 26
}

post {
  url: {{graphqlUrl}}
}

headers {
  Content-Type: application/json
}

body:graphql {
  query countryByCode($code: ID!) {
    country(code: $code) {
      code
      name
    }
  }
}

body:graphql:vars {
  {
    "code": "NO"
  }
}

body:test {
  expect(res.status).to.equal(200);
  expect(res.body.data.country.code).to.equal("NO");
}
